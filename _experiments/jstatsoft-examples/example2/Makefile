# Generated by rmake: do not edit by hand
SHELL=/bin/sh
R="$(R_HOME)/bin$(R_ARCH)/Rscript"
RM=rm

.PHONY: all
all: Makefile preview final data.rds
	
.PHONY: preview
preview: Makefile preview.pdf
	
.PHONY: final
final: Makefile final.pdf
	
data.rds: preprocess.R data.csv
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "data.rds", .script = "preprocess.R", .depends = "data.csv", .task = "all"), .Names = c(".target", ".script", ".depends", ".task"))' \
	-e '    source("preprocess.R")' \
	-e '}'
preview.pdf: preview.Rmd data.rds
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "preview.pdf", .script = "preview.Rmd", .depends = "data.rds", .format = "all", .task = "preview"), .Names = c(".target", ".script", ".depends", ".format", ".task"))' \
	-e '    rmarkdown::render("preview.Rmd", output_format = "all", output_file = "preview.pdf")' \
	-e '}'
final.pdf: final.Rmd data.rds
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "final.pdf", .script = "final.Rmd", .depends = "data.rds", .format = "all", .task = "final"), .Names = c(".target", ".script", ".depends", ".format", ".task"))' \
	-e '    rmarkdown::render("final.Rmd", output_format = "all", output_file = "final.pdf")' \
	-e '}'
.PHONY: clean
clean: 
	$(RM) data.rds
	$(RM) preview.pdf
	$(RM) final.pdf
.PHONY: clean_preview
clean_preview: 
	$(RM) preview.pdf
.PHONY: clean_final
clean_final: 
	$(RM) final.pdf
Makefile: Makefile.R
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "Makefile", .script = "Makefile.R", .depends = NULL, .task = "all"), .Names = c(".target", ".script", ".depends", ".task"))' \
	-e '    source("Makefile.R")' \
	-e '}'
