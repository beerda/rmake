# Generated by rmake: do not edit by hand
SHELL=/bin/sh
R="$(R_HOME)/bin$(R_ARCH)/Rscript"
RM=rm

.PHONY: all
all: Makefile sums.csv analysis.pdf
	
sums.csv: script.R data.csv
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "sums.csv", .script = "script.R", .depends = "data.csv", .task = "all"), .Names = c(".target", ".script", ".depends", ".task"))' \
	-e '    source("script.R")' \
	-e '}'
analysis.pdf: analysis.Rmd sums.csv
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "analysis.pdf", .script = "analysis.Rmd", .depends = "sums.csv", .format = "all", .task = "all"), .Names = c(".target", ".script", ".depends", ".format", ".task"))' \
	-e '    rmarkdown::render("analysis.Rmd", output_format = "all", output_file = "analysis.pdf")' \
	-e '}'
.PHONY: clean
clean: 
	$(RM) sums.csv
	$(RM) analysis.pdf
Makefile: Makefile.R
	$(R) -e '{' \
	-e '    params <- structure(list(.target = "Makefile", .script = "Makefile.R", .depends = NULL, .task = "all"), .Names = c(".target", ".script", ".depends", ".task"))' \
	-e '    source("Makefile.R")' \
	-e '}'
