CP=cp

.PHONY: all
all: Makefile pokus1.pdf pokus12.docx
	
pokus1%pdf pokus12%docx: pokus.Rmd data
	$(R) -e '{' \
	-e '    params <- list(.target = c("pokus1.pdf", "pokus12.docx"), .script = "pokus.Rmd", .depends = "data", .task = "all")' \
	-e '    targets <- list(pokus1.pdf = "pdf_document", pokus12.docx = "word_document")' \
	-e '    for (tg in names(targets)) {' \
	-e '        rmarkdown::render("pokus.Rmd", output_format = targets[[tg]], intermediates_dir = "./tmp", clean = FALSE, output_file = tg)' \
	-e '    }' \
	-e '}'
.PHONY: clean
clean: 
	$(RM) pokus1.pdf pokus12.docx
Makefile: Makefile.R
	$(R) -e '{' \
	-e '    params <- list(.target = "Makefile", .script = "Makefile.R", .depends = NULL, .task = "all")' \
	-e '    source("Makefile.R")' \
	-e '}'
